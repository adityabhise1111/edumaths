generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  username  String @default("user")
  email String @unique
  orgname        String?
  address        String?
  phone          String?
  description    String?
  profilePicture String?
  coverPicture   String?

}